{
  "name": "mamba-sdk-docs",
  "version": "3.0.0",
  "description": "Documentation for Mamba SDK",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "serve": "docsify serve ./docs",
    "css": "stylus -w ./docs/assets/style.styl -o ./docs/assets/style.css",
    "dev": "concurrently --kill-others \"npm run serve\" \"npm run css\"",
    "start": "cd embed && npm run start",
    "publish": "cd embed && npm run build-browser && now --prod dist/bundle.browser",
    "pkgs": "node getpkgs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/didiraja/sdk-docs-2.0.git"
  },
  "keywords": [
    "mamba",
    "sdk",
    "documentations",
    "docs"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/didiraja/sdk-docs-2.0/issues"
  },
  "homepage": "https://github.com/didiraja/sdk-docs-2.0#readme",
  "devDependencies": {
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^5.0.2",
    "docsify": "^4.10.2",
    "nodemon": "^2.0.2",
    "now": "^16.7.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "stylus": "^0.54.7"
  }
}
